<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.admindesktopapp.controllers.LogoutController"
            prefHeight="1020.0" prefWidth="1240.0">

    <Label text="Log Out"
           layoutX="570.0" layoutY="460.0"
           style="-fx-font-size: 32px;"
           fx:id="label"/>

    <Button text="Log Out"
            layoutX="570.0" layoutY="520.0"
            onAction="#logoutAction"
            style="-fx-font-size: 20px;"/>

    <Label fx:id="errorLabel" text="" layoutX="570.0" layoutY="580.0" style="-fx-text-fill: red;" />


</AnchorPane>
